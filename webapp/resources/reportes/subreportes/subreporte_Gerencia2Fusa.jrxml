	</variable>
	<variable name="TOTAL_DESCUENTO_BRUTO" class="java.lang.Integer" calculation="Sum">
		<variableExpression><![CDATA[$F{TOTAL_BRUTO} - ($F{TOTAL_BRUTO} * $F{DESCUENTO_AL_BRUTO})]]></variableExpression>
	</variable>
