	<queryString>
		<![CDATA[SELECT
  rp.PROMEDIO_MINUTOS
FROM  tbl_informacion_registradora AS ir
INNER JOIN tbl_ruta_punto AS rp ON
  rp.FK_RUTA = ir.FK_RUTA AND rp.ESTADO = 1
WHERE
  ir.FK_RUTA = $P{RUTA_PK} AND
  ir.FECHA_INGRESO = $P{FECHA_INICIAL} AND
  ir.ESTADO = 1 AND
  rp.TIPO = 3
LIMIT 1]]>
	</queryString>
